{
  "name": "@envelop/core",
  "version": "5.4.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-hive/graphql-yoga",
    "directory": "packages/envelop/core"
  },
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.cjs",
  "typings": "dist/index.d.mts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      },
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      },
      "default": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gql",
    "graphql",
    "server",
    "framework",
    "node",
    "nodejs",
    "typescript"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "tsc --pretty --noEmit",
    "watch": "tsdown --watch"
  },
  "dependencies": {
    "@envelop/instrumentation": "^1.0.0",
    "@envelop/types": "workspace:*",
    "@whatwg-node/promise-helpers": "^1.3.2",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@graphql-tools/executor": "^1.1.0",
    "@graphql-tools/schema": "10.0.30",
    "@graphql-tools/utils": "10.11.0",
    "@repeaterjs/repeater": "3.0.6",
    "graphql": "16.12.0",
    "tsdown": "^0.20.0-beta.1",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/index.d.mts"
  }
}
