{
  "name": "graphql-yoga",
  "version": "5.16.1",
  "type": "module",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-hive/graphql-yoga.git",
    "directory": "packages/graphql-yoga"
  },
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/cjs/index.js",
  "typings": "dist/typings/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "generate-graphiql-html": "node scripts/generate-graphiql-html.js",
    "inject-version": "node scripts/inject-version.js",
    "prepack": "bob prepack"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  },
  "dependencies": {
    "@envelop/core": "^5.3.0",
    "@envelop/instrumentation": "^1.0.0",
    "@graphql-tools/executor": "^1.4.0",
    "@graphql-tools/schema": "^10.0.11",
    "@graphql-tools/utils": "^10.6.2",
    "@graphql-yoga/logger": "workspace:^",
    "@graphql-yoga/subscription": "workspace:^",
    "@whatwg-node/fetch": "^0.10.6",
    "@whatwg-node/promise-helpers": "^1.2.4",
    "@whatwg-node/server": "^0.10.14",
    "lru-cache": "^10.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@envelop/disable-introspection": "8.0.0",
    "@envelop/graphql-jit": "10.0.0",
    "@envelop/live-query": "9.0.0",
    "@graphql-yoga/render-graphiql": "workspace:*",
    "@jest/globals": "^30.0.0",
    "@n1ru4l/in-memory-live-query-store": "0.10.0",
    "@repeaterjs/repeater": "^3.0.4",
    "@types/node": "24.10.0",
    "@types/pem": "1.14.4",
    "globby": "^15.0.0",
    "graphql": "16.12.0",
    "graphql-http": "^1.18.0",
    "graphql-scalars": "1.25.0",
    "graphql-sse": "2.6.0",
    "html-minifier-terser": "7.2.0",
    "json-bigint-patch": "0.0.8",
    "pem": "1.14.8",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
