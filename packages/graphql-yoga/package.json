{
  "name": "graphql-yoga",
  "version": "5.18.0",
  "type": "module",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-hive/graphql-yoga.git",
    "directory": "packages/graphql-yoga"
  },
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.cjs",
  "typings": "dist/index.d.mts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "scripts": {
    "build": "yarn generate-graphiql-html && tsdown",
    "check": "tsc --pretty --noEmit",
    "generate-graphiql-html": "bun run scripts/generate-graphiql-html.js",
    "inject-version": "bun run scripts/inject-version.js",
    "prepack": "yarn run build",
    "watch": "tsdown --watch"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  },
  "dependencies": {
    "@envelop/core": "workspace:^",
    "@envelop/instrumentation": "workspace:^",
    "@graphql-tools/executor": "^1.5.0",
    "@graphql-tools/schema": "^10.0.11",
    "@graphql-tools/utils": "^10.11.0",
    "@graphql-yoga/logger": "workspace:^",
    "@graphql-yoga/subscription": "workspace:^",
    "@whatwg-node/fetch": "^0.10.6",
    "@whatwg-node/promise-helpers": "^1.3.2",
    "@whatwg-node/server": "^0.10.14",
    "lru-cache": "^10.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@envelop/disable-introspection": "workspace:^",
    "@envelop/graphql-jit": "workspace:^",
    "@envelop/live-query": "workspace:^",
    "@graphql-yoga/graphiql": "workspace:^",
    "@jest/globals": "^30.0.0",
    "@n1ru4l/in-memory-live-query-store": "0.10.0",
    "@repeaterjs/repeater": "^3.0.4",
    "@types/node": "24",
    "@types/pem": "1.14.4",
    "globby": "^16.0.0",
    "graphql": "16.12.0",
    "graphql-http": "^1.18.0",
    "graphql-scalars": "1.25.0",
    "graphql-sse": "2.6.0",
    "html-minifier-terser": "7.2.0",
    "json-bigint-patch": "0.0.8",
    "pem": "1.14.8",
    "tsdown": "^0.20.0-beta.1",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/index.d.mts"
  }
}
