{
  "name": "@graphql-yoga/plugin-defer-stream",
  "version": "3.18.0",
  "type": "module",
  "description": "Defer/Stream plugin for GraphQL Yoga.",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-hive/graphql-yoga.git",
    "directory": "packages/plugins/defer"
  },
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.cjs",
  "typings": "dist/index.d.mts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "tsc --pretty --noEmit",
    "watch": "tsdown --watch"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0",
    "graphql-yoga": "workspace:^"
  },
  "dependencies": {
    "@graphql-tools/utils": "^10.11.0"
  },
  "devDependencies": {
    "@graphql-tools/executor-http": "^3.0.0",
    "@whatwg-node/fetch": "^0.10.6",
    "@whatwg-node/server": "^0.10.14",
    "fetch-multipart-graphql": "3.2.2",
    "graphql": "16.12.0",
    "graphql-yoga": "workspace:*",
    "tsdown": "^0.20.0-beta.1",
    "tslib": "^2.8.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/index.d.mts"
  }
}
