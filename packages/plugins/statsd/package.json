{
  "name": "@envelop/statsd",
  "version": "8.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-hive/envelop.git",
    "directory": "packages/plugins/statsd"
  },
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.cjs",
  "typings": "dist/index.d.mts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      },
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      },
      "default": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "envelop",
    "datadog",
    "statsd",
    "graphql",
    "telegraf"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "tsc --pretty --noEmit"
  },
  "peerDependencies": {
    "@envelop/core": "workspace:^",
    "hot-shots": "^11.0.0"
  },
  "dependencies": {
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@envelop/core": "workspace:^",
    "@graphql-tools/schema": "10.0.30",
    "graphql": "16.12.0",
    "hot-shots": "11.4.0",
    "tsdown": "^0.20.0-beta.1",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/index.d.mts"
  }
}
