{
  "name": "@graphql-yoga/apollo-link",
  "version": "5.3.0",
  "type": "module",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-hive/graphql-yoga.git",
    "directory": "packages/apollo-link"
  },
  "author": "Arda TANRIKULU <ardatanrikulu@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.cjs",
  "typings": "dist/index.d.mts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "graphql",
    "server",
    "graphql-yoga",
    "apollo"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "tsc --pretty --noEmit",
    "watch": "tsdown --watch"
  },
  "peerDependencies": {
    "@apollo/client": "^4.0.4",
    "graphql": "^15.2.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-tools/executor-apollo-link": "^2.0.0",
    "@graphql-tools/executor-http": "^3.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@apollo/client": "4.0.9",
    "graphql-yoga": "workspace:*",
    "tsdown": "^0.20.0-beta.1"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/index.d.mts"
  }
}
